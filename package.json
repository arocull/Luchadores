{
  "name": "luchadores",
  "version": "0.0.1",
  "description": "Luchadores is an arena-style combat game with a cast of silly and weird animals that participate in exaggerated \"wrestling\" matches.",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "pretest": "tsc",
    "test": "jest ./dist/test --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "code-quality": "eslint . --ext .js,.jsx,.ts,.tsx --output-file ./reports/codequality",
    "build": "webpack --config ./webpack/production.js",
    "watch": "webpack --config ./webpack/development.js",
    "web": "xdg-open ./dist/index.html",
    "server": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "http://git.maxocull.com/ocull/brothers/luchadores.git"
  },
  "keywords": [
    "game",
    "io",
    "fight",
    "animals",
    "wrestling",
    "arena"
  ],
  "author": "Alan O'Cull, James O'Cull, Max O'Cull",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.149",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "clean-webpack-plugin": "^3.0.0",
    "jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
